/* Series11. Даны целые числа K, N и набор из N целых чисел. Если в наборе 
имеются числа, меньшие K, то вывести True; в противном случае вывести False. */

#include "pch.h"
#include <iostream>
#include <cstdlib>

using namespace std;

int main()
{
	int K, N, r, b = 0;
	cout << "enter K,N " << endl;
	cin >> K >> N;

	for (int i = 1; i <= N; i++)
	{
		cout << "enter "<<i<<" number " << endl;
		cin >> r;
		if (r < K)
		{
			b = 1;
		}
	}
	if (b == 0)
	{
		cout << "false " << endl;
	}

	else
	{

		cout << "true " << endl;
	}
	return 0;
}
