	//  Дано четырехзначное число. Проверить истинность высказывания: «Данное число читается одинаково слева направо и справа налево».

	#include "pch.h"
	#include <iostream>
	using namespace std;

	int main()
	{
		int a;
		cout << "a";
		cin >> a;

		a = abs(a);

		cout << (a % 10 == a / 1000 & a % 100 / 10 == a % 1000 / 100) << endl;

		return 0;


	}
